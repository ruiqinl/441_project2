
CC = gcc
CFLAGS = -g -Wall -DDEBUG
LDFAGS = -lm

OBJS = peer_test.o list.o bt_parse.o debug.o input_buffer.o chunk.o sha.o packet.o spiffy.o ctr_send_recv.o process_udp.o
BINS = peer_test

.c.o:
	$(CC) $(CFLAGS) -c $<


all:$(BINS) 

peer_test: $(OBJS)
	$(CC) $(CFLAGS) $(OBJS) -o $@ $(LDFLAGS)

clean:
	rm -f $(OBJS) $(BINS)